func: quote(.First.sys)
body: function () 
body: {
body:     for (pkg in getOption("defaultPackages")) {
body:         res <- require(pkg, quietly = TRUE, warn.conflicts = FALSE, character.only = TRUE)
body:         if (!res) 
body:             warning(gettextf("package %s in options(\"defaultPackages\") was not found", sQuote(pkg)), call. = FALSE, domain = NA)
body:     }
body: }
args: NULL
retn: NULL

