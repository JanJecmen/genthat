body: function (bibtype, textVersion = NULL, header = NULL, footer = NULL, key = NULL, ..., other = list(), mheader = NULL, mfooter = NULL) 
body: {
body:     BibTeX_names <- names(tools:::BibTeX_entry_field_db)
body:     args <- c(list(...), other)
body:     if (!length(args)) 
body:         return(structure(list(), class = "bibentry"))
body:     if (any(sapply(names(args), .is_not_nonempty_text))) 
body:         stop("all fields have to be named")
body:     args <- c(list(bibtype = bibtype, textVersion = textVersion, header = header, footer = footer, key = key), list(...))
body:     args <- lapply(args, .listify)
body:     other <- lapply(other, .listify)
body:     max_length <- max(sapply(c(args, other), length))
body:     args_length <- sapply(args, length)
body:     if (!all(args_length_ok <- args_length %in% c(1L, max_length))) 
body:         warning(gettextf("Not all arguments are of the same length, the following need to be recycled: %s", paste(names(args)[!args_length_ok], collapse = ", ")), domain = NA)
body:     args <- lapply(args, function(x) rep(x, length.out = max_length))
body:     other_length <- sapply(other, length)
body:     if (!all(other_length_ok <- other_length %in% c(1L, max_length))) 
body:         warning(gettextf("Not all arguments are of the same length, the following need to be recycled: %s", paste(names(other)[!other_length_ok], collapse = ", ")), domain = NA)
body:     other <- lapply(other, function(x) rep(x, length.out = max_length))
body:     bibentry1 <- function(bibtype, textVersion, header = NULL, footer = NULL, key = NULL, ..., other = list()) {
body:         bibtype <- as.character(bibtype)
body:         stopifnot(length(bibtype) == 1L)
body:         pos <- match(tolower(bibtype), tolower(BibTeX_names))
body:         if (is.na(pos)) 
body:             stop(gettextf("%s has to be one of %s", sQuote("bibtype"), paste(BibTeX_names, collapse = ", ")), domain = NA)
body:         bibtype <- BibTeX_names[pos]
body:         rval <- c(list(...), other)
body:         rval <- rval[!sapply(rval, .is_not_nonempty_text)]
body:         fields <- tolower(names(rval))
body:         names(rval) <- fields
body:         attr(rval, "bibtype") <- bibtype
body:         .bibentry_check_bibentry1(rval)
body:         pos <- fields %in% c("author", "editor")
body:         if (any(pos)) {
body:             for (i in which(pos)) rval[[i]] <- as.person(rval[[i]])
body:         }
body:         if (any(!pos)) {
body:             for (i in which(!pos)) rval[[i]] <- as.character(rval[[i]])
body:         }
body:         attr(rval, "key") <- if (is.null(key)) 
body:             NULL
body:         else as.character(key)
body:         if (!is.null(textVersion)) 
body:             attr(rval, "textVersion") <- as.character(textVersion)
body:         if (!.is_not_nonempty_text(header)) 
body:             attr(rval, "header") <- paste(header, collapse = "\n")
body:         if (!.is_not_nonempty_text(footer)) 
body:             attr(rval, "footer") <- paste(footer, collapse = "\n")
body:         return(rval)
body:     }
body:     rval <- lapply(seq_along(args$bibtype), function(i) do.call("bibentry1", c(lapply(args, "[[", i), list(other = lapply(other, "[[", i)))))
body:     if (!.is_not_nonempty_text(mheader)) 
body:         attr(rval, "mheader") <- paste(mheader, collapse = "\n")
body:     if (!.is_not_nonempty_text(mfooter)) 
body:         attr(rval, "mfooter") <- paste(mfooter, collapse = "\n")
body:     class(rval) <- "bibentry"
body:     rval
body: }
