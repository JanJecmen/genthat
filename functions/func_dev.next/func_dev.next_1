func: quote(dev.next)
body: function (which = dev.cur()) 
body: {
body:     if (!exists(".Devices")) 
body:         .Devices <- list("null.device")
body:     num.device <- .External(C_devnext, as.integer(which))
body:     names(num.device) <- .Devices[[num.device]]
body:     num.device
body: }
args: NULL
retn: structure(2L, .Names = "pdf")

