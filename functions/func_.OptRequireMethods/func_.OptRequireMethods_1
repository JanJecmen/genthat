func: quote(.OptRequireMethods)
body: function () 
body: {
body:     if ("methods" %in% getOption("defaultPackages")) {
body:         res <- require("methods", quietly = TRUE, warn.conflicts = FALSE, character.only = TRUE)
body:         if (!res) 
body:             warning("package \"methods\" in options(\"defaultPackages\") was not found", call. = FALSE)
body:     }
body: }
args: NULL
