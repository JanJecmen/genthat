body: function (x) 
body: {
body:     v <- paste(R.version[c("major", "minor")], collapse = ".")
body:     expand <- function(x, spec, expansion) gsub(paste0("(^|[^%])(%%)*%", spec), sprintf("\\1\\2%s", expansion), x)
body:     x <- expand(x, "V", v)
