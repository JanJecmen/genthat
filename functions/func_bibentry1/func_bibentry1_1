func: quote(bibentry1)
body: function (bibtype, textVersion, header = NULL, footer = NULL, key = NULL, ..., other = list()) 
body: {
body:     bibtype <- as.character(bibtype)
body:     stopifnot(length(bibtype) == 1L)
body:     pos <- match(tolower(bibtype), tolower(BibTeX_names))
body:     if (is.na(pos)) 
body:         stop(gettextf("%s has to be one of %s", sQuote("bibtype"), paste(BibTeX_names, collapse = ", ")), domain = NA)
body:     bibtype <- BibTeX_names[pos]
body:     rval <- c(list(...), other)
body:     rval <- rval[!sapply(rval, .is_not_nonempty_text)]
body:     fields <- tolower(names(rval))
body:     names(rval) <- fields
body:     attr(rval, "bibtype") <- bibtype
body:     .bibentry_check_bibentry1(rval)
body:     pos <- fields %in% c("author", "editor")
body:     if (any(pos)) {
body:         for (i in which(pos)) rval[[i]] <- as.person(rval[[i]])
body:     }
body:     if (any(!pos)) {
body:         for (i in which(!pos)) rval[[i]] <- as.character(rval[[i]])
body:     }
body:     attr(rval, "key") <- if (is.null(key)) 
body:         NULL
body:     else as.character(key)
body:     if (!is.null(textVersion)) 
body:         attr(rval, "textVersion") <- as.character(textVersion)
body:     if (!.is_not_nonempty_text(header)) 
body:         attr(rval, "header") <- paste(header, collapse = "\n")
body:     if (!.is_not_nonempty_text(footer)) 
body:         attr(rval, "footer") <- paste(footer, collapse = "\n")
body:     return(rval)
body: }
