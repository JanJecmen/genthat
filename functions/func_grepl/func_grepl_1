func: quote(grepl)
body: function (pattern, x, ignore.case = FALSE, perl = FALSE, fixed = FALSE, useBytes = FALSE) 
body: {
body:     if (!is.character(x)) 
body:         x <- as.character(x)
body:     .Internal(grepl(as.character(pattern), x, ignore.case, FALSE, perl, fixed, useBytes, FALSE))
body: }
args: list("^[[:space:]]*(R|[[:alpha:]][[:alnum:].]*[[:alnum:]])([[:space:]]*\\(([^) ]+)[[:space:]]+([^) ]+)\\))?[[:space:]]*$", " R (>= 2.13.0) ")
retn: TRUE

